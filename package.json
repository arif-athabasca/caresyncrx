    "validate:all": "node scripts/final-validation.cjs",
    "test:app": "node scripts/test-app-functionality.cjs",
    "detect:circular": "node scripts/detect-circular-deps.cjs",
    "cleanup:empty": "node scripts/cleanup-empty-files.cjs",
    "cleanup:redundant": "node scripts/cleanup-redundant-files.cjs",
    "cleanup:duplicate": "node scripts/cleanup-duplicate-scripts.cjs",
    "cleanup:final": "node scripts/final-cleanup.cjs",
    "cleanup:all": "npm run cleanup:redundant && npm run cleanup:duplicate && npm run cleanup:empty && npm run cleanup:final",
    "test:auth": "powershell -ExecutionPolicy Bypass -File .\\Test-AuthSystem.ps1 -Full",
    "test:auth:integration": "powershell -ExecutionPolicy Bypass -File .\\Test-AuthSystem.ps1 -Integration",
    "test:auth:realworld": "powershell -ExecutionPolicy Bypass -File .\\Test-AuthSystem.ps1 -RealWorld"
